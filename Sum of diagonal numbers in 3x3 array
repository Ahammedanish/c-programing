#include<stdio.h>
int main()
{
    int a[3][3],i,j,n,s;
    printf("enter the value");
    for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            printf("a[%d][%d]",i,j);
            scanf("%d",&a[i][j]);
        }
    }
    printf("enter the matrix \n");
    for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            printf("\t%d",a[i][j]);
        }
        printf("\n\n");
        
    }
    s=a[0][0]+a[1][1]+a[2][2];
    printf("%d",s);
    return 0;
}
